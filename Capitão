public class Capitao implements Hierarquia {
    private TipoDeArma tipoDeArma;

    public Capitao(TipoDeArma tipoDeArma) {
        this.tipoDeArma = tipoDeArma;
    }

    private int getNivel() {
        return 2;
    }

    public boolean superiorA(Hierarquia obj) {
        if (obj instanceof Capitao) return false;
        return this.getNivel() > ((PessoaMilitar)obj) .getNivel();
    }

    public boolean inferiorA(Hierarquia obj) {
        if (obj instanceof Capitao) return false;
        return this.getNivel() < ((PessoaMilitar)obj) .getNivel();
    }

    public boolean equivalenteA(Hierarquia obj) {
        return obj instanceof Capitao;
    }
    }
